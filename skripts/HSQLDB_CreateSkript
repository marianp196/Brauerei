CREATE TABLE Bier (
	id INT NOT NULL,
	name VARCHAR(50),
	erstellungsZeitpunkt BIGINT,
	beschreibung VARCHAR(1000),	
	PRIMARY KEY(id)   
);
CREATE TABLE BrauprozessElemente (
	id INT NOT NULL,
	bierId INT,	
	name VARCHAR(50),
	typ int,
	zeit INT,
	minTemp DECIMAL,
	maxTemp DECIMAL,
	erstellungsZeitpunkt BIGINT,
	beschreibung VARCHAR(500),	
	PRIMARY KEY(id)   
);
CREATE TABLE ElementTyp (
	id INT NOT NULL,
	name VARCHAR(50),
	PRIMARY KEY(id)
);

